				"burnNFT(uint256)": {
					"details": "Public function to burn an NFT. The corresponding supply is reduced, and the owner's NFT status is updated.",
					"params": {
						"tokenId": "The ID of the NFT to be burned."
					}
				},
				"constructor": {
					"details": "Constructor to initialize the NFT contract with the base URI, token contract, required token balance, vault address, and iface address.",
					"params": {
						"_ifaceAddress": "The address of the iface contract for minting ERC20 tokens.",
						"_setTokenBalanceRequired": "The minimum balance of tokens required to mint an NFT.",
						"_setVault": "The address where transaction fees will be sent.",
						"_tokenContract": "The address of the required ERC20 token contract.",
						"_tokenURI": "The base URI for the NFTs."
					}
				},
				"ethRescue(address,uint256)": {
					"details": "Function to rescue Ether sent to the contract by mistake.",
					"params": {
						"_dest": "The address to send the rescued Ether to.",
						"_etherAmount": "The amount of Ether to rescue."
					}
				},
				"getApproved(uint256)": {
					"details": "See {IERC721-getApproved}."
				},
				"getRoleAdmin(bytes32)": {
					"details": "Returns the admin role that controls `role`. See {grantRole} and {revokeRole}. To change a role's admin, use {_setRoleAdmin}."
				},
				"grantRole(bytes32,address)": {
					"details": "Grants `role` to `account`. If `account` had not been already granted `role`, emits a {RoleGranted} event. Requirements: - the caller must have ``role``'s admin role. May emit a {RoleGranted} event."
				},
				"hasRole(bytes32,address)": {
					"details": "Returns `true` if `account` has been granted `role`."
				},
				"hashUserAddress(uint256)": {
					"details": "Function to hash the user's address with a tokenId. Used for generating unique URIs.",
					"params": {
						"eid": "The tokenId to hash with the user's address."
					},
					"returns": {
						"_0": "The resulting hash as a bytes32 value."
					}
				},
				"hashUserAddress2(string,uint256)": {
					"details": "Function to hash the user's address with a level and tokenId.",
					"params": {
						"_eid": "The tokenId to hash with the user's address and level.",
						"_level": "The level to hash with the user's address and tokenId."
					},
					"returns": {
						"_0": "The resulting hash as a bytes32 value."
					}
				},
				"isApprovedForAll(address,address)": {
					"details": "See {IERC721-isApprovedForAll}."
				},
				"mintNFT(uint256)": {
					"details": "Public function to mint an NFT after the initial token minting phase is over. No associated ERC20 tokens are minted in this function.",
					"params": {
						"_level": "The level of the NFT to be minted (1 for Gold, 2 for Silver, 3 for Bronze)."
					},
					"returns": {
						"_0": "The tokenId of the newly minted NFT."
					}
				},
				"mintNFTAndToken(uint256)": {
					"details": "Public function to mint an NFT and associated ERC20 tokens based on the specified level. Requires the contract to be unpaused and checks various conditions based on the level.",
					"params": {
						"_level": "The level of the NFT to be minted (1 for Gold, 2 for Silver, 3 for Bronze)."
					},
					"returns": {
						"_0": "The tokenId of the newly minted NFT."
					}
				},
				"moveERC20(address,address,uint256)": {
					"details": "Function to rescue ERC20 tokens sent to the contract by mistake.",
